{
	"name": "fastifystarter",
	"version": "1.0.0",
	"description": "This is a starter template for fastify",
	"main": "server.js",
	"type": "module",
	"author": "AxelTahmid",
	"license": "Apache 2.0",
	"engines": {
		"npm": ">=11.0.0",
		"node": ">=22.14.0"
	},
	"packageManager": "yarn@4.6.0",
	"scripts": {
		"build": "tsc -p tsconfig.json",
		"start": "tsx src/server.ts",
		"dev": "tsx watch src/server.ts",
		"knx": "NODE_OPTIONS='--import tsx/esm' knex",
		"knex": "tsx ./node_modules/knex/bin/cli.js --knexfile=./src/database/knexfile",
		"migrate": "yarn knex -- migrate:latest --verbose",
		"migrate-create": "yarn knex -- migrate:make --verbose",
		"seed": "yarn knex -- seed:run --verbose",
		"seed-create": "yarn knex -- seed:make --verbose",
		"rollback": "yarn knex -- migrate:rollback --verbose",
		"lint-fix": "yarn dlx @biomejs/biome check --apply .",
		"lint-check": "yarn dlx @biomejs/biome check .",
		"deps": "yarn upgrade-interactive"
	},
	"dependencies": {
		"@aws-sdk/client-s3": "^3.750.0",
		"@fastify/cors": "^10.1.0",
		"@fastify/formbody": "^8.0.2",
		"@fastify/helmet": "^13.0.1",
		"@fastify/jwt": "^9.0.4",
		"@fastify/multipart": "^9.0.3",
		"@fastify/sensible": "^6.0.3",
		"@fastify/under-pressure": "^9.0.3",
		"bcryptjs": "^3.0.2",
		"bullmq": "^5.41.5",
		"close-with-grace": "^2.2.0",
		"dotenv": "^16.4.7",
		"fastify": "^5.2.1",
		"fastify-plugin": "^5.0.1",
		"ioredis": "^5.5.0",
		"knex": "^3.1.0",
		"nodemailer": "^6.10.0",
		"ofetch": "^1.4.1",
		"pg": "^8.13.3"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@fastify/type-provider-typebox": "^5.1.0",
		"@sinclair/typebox": "^0.34.28",
		"@types/node": "^22.13.5",
		"@types/nodemailer": "^6.4.17",
		"@types/pg": "^8.11.11",
		"pino-pretty": "^13.0.0",
		"tsx": "^4.19.3",
		"typescript": "^5.7.3"
	}
}